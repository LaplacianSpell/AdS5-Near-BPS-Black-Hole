          RowBox[{
           RowBox[{"met", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "j"}], "]"}], "]"}], " ", "=", 
           RowBox[{
            RowBox[{"met", "[", 
             RowBox[{"[", 
