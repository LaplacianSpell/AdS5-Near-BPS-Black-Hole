     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "<=", " ", "dim"}], ",", " ", 
       RowBox[{"i", "++"}], ",", " ", "\[IndentingNewLine]", "\t", 
       RowBox[{
        RowBox[{
         RowBox[{"met", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", "i"}], "]"}], "]"}], " ", "=", " ", 
         RowBox[{"Coefficient", "[", 
          RowBox[{"lineelem", ",", 
           RowBox[{"oneforms", "[", 
            RowBox[{"[", "i", "]"}], "]"}], ",", "2"}], "]"}]}], "  ", ";", 
        "\[IndentingNewLine]", "\t", 
        RowBox[{"For", "[", 
         RowBox[{
